 
# Discord Bot Token (replace with your actual token)
DISCORD_TOKEN=YOUR_DISCORD_TOKEN_HERE

# Bot command prefix
COMMAND_PREFIX=!

# Directory for saving channel messages
CHANNEL_MESSAGES_DIR=channel_messages

# File name format for single channel messages
SINGLE_CHANNEL_FILE_FORMAT={channel_name}_messages.json

# Required permission to use bot commands
REQUIRED_PERMISSION=administrator

# Message history limit (0 for no limit)
MESSAGE_HISTORY_LIMIT=0

# Encoding for file operations
FILE_ENCODING=utf-8

# JSON indentation for saved files
JSON_INDENT=4
